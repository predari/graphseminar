\newcommand{\HUB@MFrame}[1][0mm]{%
  \fill[HUBmframe] (\HUB@MFllx,\HUB@MFury)
    -- (\HUB@MFllx,\HUB@MFlly) -- (\HUB@MFurx,\HUB@MFlly)
    -- (\HUB@MFurx,\HUB@MFury) -- (\HUB@MFllx,\HUB@MFury)
    -- (\HUB@MFllx+\HUB@Framewd,\HUB@MFury-\HUB@Framewd)
    -- (\HUB@MFurx-2\HUB@Framewd,\HUB@MFury-\HUB@Framewd)
    arc( 90:  0:\HUB@Framewd)
    -- (\HUB@MFurx-\HUB@Framewd,\HUB@MFlly+\HUB@Framewd+#1)
    -- (2\HUB@Framewd,\HUB@MFlly+\HUB@Framewd+#1)
    arc(270:180:\HUB@Framewd)
    -- (\HUB@MFllx+\HUB@Framewd,\HUB@MFury-\HUB@Framewd) -- cycle;
  }
\newcommand{\HUB@MFrameMOD}[1][0mm]{%
  \fill[HUBmframe] (0mm,\paperheight)
    -- (0mm,0mm) -- (\paperwidth,0mm)
    -- (\paperwidth,\paperheight) -- (0mm,\paperheight)
    -- (\HUB@Framewd,\paperheight-\HUB@Framewd)
    -- (\paperwidth-2\HUB@Framewd,\paperheight-\HUB@Framewd)
    arc( 90:  0:\HUB@Framewd)
    -- (\paperwidth-\HUB@Framewd,\HUB@Framewd+#1)
    -- (2\HUB@Framewd,\HUB@Framewd+#1)
    arc(270:180:\HUB@Framewd)
    -- (\HUB@Framewd,\paperheight-\HUB@Framewd) -- cycle;
  }
\newcommand{\HUB@MFrameRA}[1][0mm]{%
  \draw[HUBrframe,line width=\HUB@RFramewd] (\HUB@MFllx+#1,\HUB@MFury)
    -- (\HUB@MFllx+#1,\HUB@MFlly+\HUB@Framewd)
    arc(180:270:\HUB@Framewd)
    -- (\HUB@MFurx+#1,\HUB@MFlly)
    -- (\HUB@MFurx+#1,\HUB@MFury-\HUB@Framewd)
    arc(  0: 90:\HUB@Framewd) -- cycle;
  }
