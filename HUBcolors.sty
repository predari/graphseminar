\newcommand{\SW}{%
  \HUB@bwtrue
  \colorlet{HUBheadbg}{HUBblack50}
  \ifHUB@De\let\HUB@logo=\HUB@logo@bwD\else\let\HUB@logo=\HUB@logo@bwE\fi
  \renewcommand{\labelitemi}{\resizebox{1ex}{1ex}{\HUB@marksymbol[HUBblack]}}
}
\let\BW=\SW
\newcommand{\Color}{%
  \HUB@bwfalse
  \colorlet{HUBheadbg}{HUBblue}
  \ifHUB@De\gdef\HUB@logo{\HUB@logo@clrD}
    \else\gdef\HUB@logo{\HUB@logo@clrE}
    \fi
  \let\labelitemi=\HUB@mark
}
\let\Farbe=\Color
